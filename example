(*
Hello_world
*)
const MAX = 1000;
var i,j,n,s:longint;
begin
  readln(n);//hello_world
  s:=0;
  for i:=1 to n do begin
    opt[i]:=1;
    {
    hello_world
    }
    for j:=1 to i-2 do
      inc(opt[i],opt[j]);
    inc(s,opt[i]);
  end;
  (* hello_world *)
  writeln(s);
end.